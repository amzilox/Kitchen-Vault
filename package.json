{
  "name": "kitchenvault",
  "version": "1.0.0",
  "description": "kitchenvault | Recipe finder app",
  "default": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "parcel-init": "parcel index.html",
    "start": "npm-run-all --parallel watch:sass parcel-init devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build:js": "parcel build index.html --dist-dir ./dist",
    "build": "npm-run-all build:css build:js"
  },
  "author": "amzilox",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  },
  "dependencies": {
    "core-js": "^3.38.0",
    "fraction.js": "^4.3.7",
    "gsap": "^3.12.5",
    "parcel": "^2.12.0",
    "regenerator-runtime": "^0.14.1",
    "scrollmagic": "^2.0.8",
    "scrollmagic-plugin-gsap": "^1.0.4"
  }
}
